{
  "name": "web-app-boilerplate",
  "version": "1.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS, based on work by Christoph von Gellhorn",
  "author": "Christoph von Gellhorn, Igor Zinken",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "test": "webpack -p --progress --mode development --config webpack.config.test.js && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "ejs-compiled-loader": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "chai": "^1.9.2",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "mocha": "^2.2.5",
    "mocha-loader": "^1.1.3",
    "node-sass": "4.8.3",
    "sass-loader": "7.0.1",
    "sinon": "^2.3.4",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2"
  }
}
